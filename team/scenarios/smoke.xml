        <regexps>
            <regexp>.*\.jpg</regexp>
            <regexp>.*\.jpeg</regexp>
            <regexp>.*\.gif</regexp>
            <regexp>.*\.png</regexp>
            <regexp>.*\.bmp</regexp>
            <regexp>.*\.css</regexp>
            <regexp>.*\.js</regexp>
            <regexp>.*\.swf</regexp>
            <regexp>.*\.ico</regexp>
            <regexp>.*\.jar</regexp>
            <regexp>.*\.class</regexp>
            <regexp>.*\.properties</regexp>
        </regexps>
    </request-path-exclusion-filter>
    <!--****** END  EXCLUSION FILTER ******-->
    <!--****** START DYNATRACE MONITORING ******-->
    <dynatrace-monitoring/>
    <!--****** END  DYNATRACE MONITORING ******-->
